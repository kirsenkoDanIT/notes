
<div class="container vh-100 d-flex flex-column bg-light border border-primary">
    <article class=" w-100 text-center link-block">
        <a href="/" class="btn btn-success w-100 fixed-top">Home</a>
    </article>
    <h1>Lists</h1>
    <% newLists.forEach((item, i)=> {%>
    <div class="card border border-primary my-1">
        <div class="card-body">
            <button type="button" class="close" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <p class="card-text"><%= item.text%></p>
            <a href="#" class="btn btn-primary w-50">View</a>
        </div>
    </div>
    <% })%>
    <article class=" w-100 text-center link-block">
        <a href="/create_notes" class="btn btn-primary w-100 fixed-bottom">New list</a>
    </article>
</div>

